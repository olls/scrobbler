- if (scrobbling) {
-   var title = 'Scrobbling ' + scrobbling.title;
- } else {
-   var title = 'Scobble a Record';
- }

html
  head
    title!= title
  body
    h1!= title
    form
      label Discogs ID:
        input(name='id')
      br
      input(type='submit')
    ul.record
      each track in tracks ? tracks : []
        li= track.title
